<?xml version="1.0"?>
<tool id="fa_gc_content_2" name="Plot weather">
  <description>using R to plot weather data</description>
  <command interpreter="bash">boxplot_wei.sh $surveyacebData $output</command>
  <inputs>
     <param name="surveyacebdata" type="data" format=".aceb" label="Input Survey.aceb Data" />
  </inputs>
  
  <outputs>
         <date format="png" name="boxplot" label="Boxplot"/>
  </outputs>
  
  <help>
This tool generate a boxplot for specific weather site.
  </help>

</tool>